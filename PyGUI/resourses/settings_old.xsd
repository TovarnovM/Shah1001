        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="conflict:DangerFactor"/>
        </xsd:complexContent>
    </xsd:complexType>

    
    
    <xsd:complexType name="TargetDistanceDF">
        <xsd:annotation>
            <xsd:documentation> Фактор опасности дальность </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="conflict:DangerFactor"/>
        </xsd:complexContent>
    </xsd:complexType>

    
    
    <xsd:complexType name="CurrentMissDF">
        <xsd:annotation>
            <xsd:documentation> Фактор опасности текущий промах </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="conflict:DangerFactor"/>
        </xsd:complexContent>
    </xsd:complexType>

    

