	<xsd:complexType name="CentralComputer">
		<xsd:annotation>
			<xsd:documentation> Бортовой цифровой вычислитель </xsd:documentation>
		</xsd:annotation>		
		<xsd:sequence>
			<xsd:element name="algorithms_v" type="conflict:ControlAlgorithm" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation> Вектор алгоритмов управления </xsd:documentation>
				</xsd:annotation>		
			</xsd:element>			
		</xsd:sequence>
	</xsd:complexType>
	

	
	<!--  HOMING HEADS  -->
	
	
	<xsd:complexType name="HomingHead" abstract="true">
		<xsd:annotation>
			<xsd:documentation> Абстрактный класс головки самонаведения </xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="friend_detection" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation> Возможность определять свой/чужой </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	
	

	
	<xsd:complexType name="HomingHeadSimple">
		<xsd:annotation>
			<xsd:documentation> Базовая модель головки самоноведения </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="conflict:HomingHead">
				<xsd:sequence>			
					<xsd:element name="phi_h" type="conflict:Limits">	
						<xsd:annotation>
							<xsd:documentation> Диапазон горизонтального угла прокачки, град </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="phi_v" type="conflict:Limits">	
						<xsd:annotation>
							<xsd:documentation> Диапазон вертикального угла прокачки, град </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="distance" type="conflict:incPositiveDouble" use="required">
					<xsd:annotation>
						<xsd:documentation> Дальность видимости ГСН </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>	
				<xsd:attribute name="target_type" type="conflict:ObjectType" default="_other">
					<xsd:annotation>
						<xsd:documentation> Тип искомой цели </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>	
			</xsd:extension>			
		</xsd:complexContent>	
	</xsd:complexType>
		
	
	
	<xsd:complexType name="HomingHeadInfrared">
		<xsd:annotation>
			<xsd:documentation> Инфракрасная головка самоноведения </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="conflict:HomingHead">
				<xsd:sequence>			
					<xsd:element name="phi_h" type="conflict:Limits">	
						<xsd:annotation>
							<xsd:documentation> Диапазон горизонтального угла прокачки </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="phi_v" type="conflict:Limits">	
						<xsd:annotation>
							<xsd:documentation> Диапазон вертикального угла прокачки </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="front_distance" type="conflict:incPositiveDouble" use="required">
					<xsd:annotation>
						<xsd:documentation> Дальность при фронтальном наблюдении </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="bottom_distance" type="conflict:incPositiveDouble" use="required">
					<xsd:annotation>
						<xsd:documentation> Дальность при тыловом наблюдении </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>				
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
			
	<!--  HOMING HEADS  END  -->
	
	
	
	<!--  RADARS  -->
	
	<xsd:complexType name="Radar" abstract="true">
		<xsd:annotation>
			<xsd:documentation> Базовый класс для всех радаров </xsd:documentation>
		</xsd:annotation>	
		<xsd:sequence>
			<xsd:group ref="conflict:Statable"/>							
		</xsd:sequence>	
		<xsd:attribute name = "scan_freq" type = "conflict:exPositiveDouble" use="required">
			<xsd:annotation>
				<xsd:documentation> Частота сканирования пространства (Гц) </xsd:documentation>
				<xsd:appinfo source="Xsd2CppGen">
					<genParam trancatedInResult="True"/>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>		
	</xsd:complexType>
	
	
	
	<xsd:complexType name="RadarSimple">
		<xsd:annotation>
			<xsd:documentation> Простая модель радара. Ищет цели с заданной частотой в заданном радиусе </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="conflict:Radar">							
				<xsd:attribute name = "vision_radius" type = "conflict:exPositiveDouble" use="required">
					<xsd:annotation>
						<xsd:documentation> Радиус обнаружения цели (м) </xsd:documentation>
						<xsd:appinfo source="Xsd2CppGen">
							<genParam trancatedInResult="True"/>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>				
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	
	
	<xsd:complexType name="RadarInfrared">
		<xsd:annotation>
			<xsd:documentation> Тепловизионный радар </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="conflict:Radar">				
				<xsd:attribute name="front_distance" type="conflict:incPositiveDouble" use="required">
					<xsd:annotation>
						<xsd:documentation> Дальность при фронтальном наблюдении </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="bottom_distance" type="conflict:incPositiveDouble" use="required">
					<xsd:annotation>
						<xsd:documentation> Дальность при тыловом наблюдении </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<!--  RADARS END  -->
	
	
	<!--  MEASURERS -->
	
	<xsd:complexType name="Measurer" abstract="true">
		<xsd:annotation>
            <xsd:documentation> Абстрактный тип измерителя </xsd:documentation>
        </xsd:annotation>
		<xsd:sequence>
			<xsd:element name="measured_state" type="conflict:State" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> Измеренное состояние объекта </xsd:documentation>
				</xsd:annotation>
			</xsd:element>		
		</xsd:sequence>		
	</xsd:complexType>
	
	
	
	<xsd:complexType name="StrapDownInertialNavigationsSystem">
		<xsd:annotation>
            <xsd:documentation> БИНС </xsd:documentation>
        </xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="conflict:Measurer">
				<xsd:sequence>
					<xsd:element name="a0" type="conflict:RVector" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation> ошибка выставки ДЛУ (м/с^2) </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="w0" type="conflict:RVector" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation> дрейф ДУС (рад/с) </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<!--  MEASURERS END-->
	
	
	
	<!--  SELF DESTRUCTION -->
	<xsd:complexType name="DestructionFactor" abstract = "true">
		<xsd:annotation>
            <xsd:documentation> Класс интерфейс для всех факторов самоликвидации </xsd:documentation>
        </xsd:annotation>		
	</xsd:complexType>
	
	
	
	<xsd:complexType name="DestructionFactorLifeTime">
		<xsd:annotation>
            <xsd:documentation> Самоликвидация по продолжительности автономного существования </xsd:documentation>
        </xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="conflict:DestructionFactor">
				<xsd:attribute name="max_time" type="conflict:exPositiveDouble" use="required">
					<xsd:annotation>
						<xsd:documentation> Максимальная продолжитительность автономного существования (сек) </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	
	
	<xsd:complexType name="DestructionFactorDistance">
		<xsd:annotation>
            <xsd:documentation> Самоликвидация по максимальному перемещению </xsd:documentation>
        </xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="conflict:DestructionFactor">
				<xsd:attribute name="max_distance" type="conflict:exPositiveDouble" use="required">
					<xsd:annotation>
						<xsd:documentation> Максимальная дальность движения (м) </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	
	
	<xsd:complexType name="DestructionFactorLostTarget">
		<xsd:annotation>
            <xsd:documentation> Самоликвидация при отсутствии повторного завхвата цели </xsd:documentation>
        </xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="conflict:DestructionFactor">
				<xsd:attribute name="max_time" type="conflict:exPositiveDouble" use="required">
					<xsd:annotation>
						<xsd:documentation> Максимальное ожидание повторного захвата (с) </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	
	
	<xsd:complexType name="SelfDestructionAlgorithm">
		<xsd:annotation>
            <xsd:documentation> Алгоритм самоликвидации </xsd:documentation>
        </xsd:annotation>		
		<xsd:sequence>
			<xsd:element name="factors" type="conflict:DestructionFactor" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation> Список факторов для ликвидации </xsd:documentation>
				</xsd:annotation>
			</xsd:element>				
		</xsd:sequence>					
	</xsd:complexType>
	
	<!--  SELF DESTRUCTION -->
	
 
	<xsd:complexType name="ComplexationsAlgorithm">
		<xsd:annotation>
            <xsd:documentation> Алгоритм комплексаирования информации датчиков </xsd:documentation>
        </xsd:annotation>
		<xsd:sequence>
			<xsd:element name="measured_state" type="conflict:State" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> Комплексированное состояние объекта </xsd:documentation>
				   <xsd:appinfo source="Xsd2CppGen">						
						<dialog show = "False"/>						
				   </xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	
	
	<xsd:complexType name="MeasuringSystem">
		<xsd:annotation>
            <xsd:documentation> Система измерения состояния объекта </xsd:documentation>
        </xsd:annotation>
		<xsd:sequence>
			<xsd:element name="measurers_v" type="conflict:Measurer" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation> Набор датчиков </xsd:documentation>
				</xsd:annotation>
			</xsd:element>	
			<xsd:element name="measured_state" type="conflict:State" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> Измеренное состояние объекта </xsd:documentation>
				   <xsd:appinfo source="Xsd2CppGen">						
						<dialog show = "False"/>						
				   </xsd:appinfo>
				</xsd:annotation>
			</xsd:element>		
			<xsd:element name="complex_alg" type="conflict:ComplexationsAlgorithm" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> Алгоритм комплексирования </xsd:documentation>
				</xsd:annotation>
			</xsd:element>				
		</xsd:sequence>
	</xsd:complexType>
	
	
		
    <xsd:complexType name="DangerFactor" abstract="true">
        <xsd:annotation>
            <xsd:documentation> Абстрактный фактор опасности </xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="relative_danger" type="conflict:exPositiveInt" use="required">
            <xsd:annotation>
                <xsd:documentation> Относительная опасность </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    
    
    <xsd:complexType name="FlyinTimeDF">
