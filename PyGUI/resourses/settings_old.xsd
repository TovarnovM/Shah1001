    <xsd:complexType name="Zone">	
		<xsd:annotation>
            <xsd:documentation> Зона </xsd:documentation>
			<xsd:appinfo source="Xsd2CppGen">
				<genParam noInheritance="True"/>
			</xsd:appinfo>
        </xsd:annotation>
        <xsd:attribute name="radius" type="xsd:double" use="required"/>
    </xsd:complexType>
    	
	
	
	<xsd:complexType name="HitProbability">
		<xsd:annotation>
            <xsd:documentation> Вероятность попадания по данному типу </xsd:documentation>
			<xsd:appinfo source="Xsd2CppGen">
				<genParam noInheritance="True"/>
			</xsd:appinfo>
        </xsd:annotation>
        <xsd:attribute name="target_type" type="conflict:ObjectType" use="required">
			<xsd:annotation>
				<xsd:documentation> Тип цели </xsd:documentation>
				<xsd:appinfo source="Xsd2CppGen">
					<genParam trancatedInResult="True"/>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
        <xsd:attribute name="value" type="xsd:double" use="required">
			<xsd:annotation>
				<xsd:documentation> Вероятность попадания </xsd:documentation>
				<xsd:appinfo source="Xsd2CppGen">
					<genParam trancatedInResult="True"/>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
    </xsd:complexType>
	
	
	
	<xsd:complexType name="HitProbabilities">
		<xsd:annotation>
            <xsd:documentation> Вектор вероятностей попадания для всех типов </xsd:documentation>
			<xsd:appinfo source="Xsd2CppGen">
				<genParam noInheritance="True"/>
			</xsd:appinfo>
        </xsd:annotation>
		<xsd:attribute name="other" type="conflict:Percent">
			<xsd:annotation> <xsd:documentation> Вероятность поражения остальных типов, [0-1] </xsd:documentation> </xsd:annotation>		
		</xsd:attribute>
		<xsd:attribute name="big_missile" type="conflict:Percent">
			<xsd:annotation> <xsd:documentation> Вероятность поражения большой ракеты, [0-1] </xsd:documentation> </xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="small_missile" type="conflict:Percent">
			<xsd:annotation> <xsd:documentation> Вероятность поражения малой ракеты, [0-1] </xsd:documentation> </xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="bomber" type="conflict:Percent">
			<xsd:annotation> <xsd:documentation> Вероятность поражения бомбардировщика, [0-1] </xsd:documentation> </xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="fighter" type="conflict:Percent">
			<xsd:annotation> <xsd:documentation> Вероятность поражения истребителя, [0-1] </xsd:documentation> </xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="big_ship" type="conflict:Percent">
			<xsd:annotation> <xsd:documentation> Вероятность поражения большого корабля, [0-1] </xsd:documentation> </xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="small_ship" type="conflict:Percent">
			<xsd:annotation> <xsd:documentation> Вероятность поражения малого корабля, [0-1] </xsd:documentation> </xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="aerocarrier" type="conflict:Percent">
			<xsd:annotation> <xsd:documentation> Вероятность поражения авианосца, [0-1] </xsd:documentation> </xsd:annotation>
		</xsd:attribute>
        <!--xsd:sequence>
            <count of TargetType values >
            <xsd:element name="hit_probability" type="conflict:HitProbability" minOccurs="8" maxOccurs="8"/>
        </xsd:sequence-->
        <xsd:attribute name="id" type="xsd:ID"/>
    </xsd:complexType>
	
	
	
    <xsd:complexType name="FiringChannel">
        <xsd:annotation>
            <xsd:documentation> Стрельбовой канал </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:group ref="conflict:Statable"/> 
            <xsd:element name="noescape_zone" type="conflict:Zone">
                <xsd:annotation>
                    <xsd:documentation> Зона поражения </xsd:documentation>
                    <xsd:appinfo source="Xsd2CppGen">
                        <genParam trancatedInResult="True"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>			
			<xsd:element name="hit_probabilities" type="conflict:HitProbabilities">			
				<xsd:annotation>
					<xsd:documentation> Веротяности поражения всех типов целей </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
        <xsd:attribute name="reload_time" type="conflict:incPositiveDouble" default="15.0">
            <xsd:annotation>
                <xsd:documentation> Время для перезаряда боеукладки </xsd:documentation>
				<xsd:appinfo source="Xsd2CppGen">
					<genParam trancatedInResult="True"/>
				</xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
		<xsd:attribute name="volley_size" type="conflict:exPositiveDouble" default="1">
            <xsd:annotation>
                <xsd:documentation> Число снарядов на одну цель </xsd:documentation>
				<xsd:appinfo source="Xsd2CppGen">
					<genParam trancatedInResult="True"/>
				</xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
		<xsd:attribute name="delay" type="conflict:incPositiveDouble" default="5.0">
            <xsd:annotation>
                <xsd:documentation> Задержка между стрельбами </xsd:documentation>
				<xsd:appinfo source="Xsd2CppGen">
					<genParam trancatedInResult="True"/>
				</xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>				
		<xsd:attribute name="acceleration" type="conflict:incPositiveDouble" default="50">
            <xsd:annotation>
                <xsd:documentation> Скорость вылета снаряда </xsd:documentation>
				<xsd:appinfo source="Xsd2CppGen">
					<genParam trancatedInResult="True"/>
				</xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>		
    </xsd:complexType>
	
	
	
	<xsd:complexType name="Ammunition">
		<xsd:annotation>
            <xsd:documentation> Модель боеукладки </xsd:documentation>
        </xsd:annotation>
		<xsd:sequence>
			<xsd:element name="prototype" type="conflict:Missile">
				<xsd:annotation>
					<xsd:documentation> Прототип ракеты из арсенала </xsd:documentation>
					<xsd:appinfo source="Xsd2CppGen">
						<genParam>
							<prototype destListName="missiles" global="true"/>
						</genParam>
					</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="capacity" type="conflict:exPositiveDouble" use="required">
			<xsd:annotation>
				<xsd:documentation> Число ракет в арсенале </xsd:documentation>
			</xsd:annotation>		
		</xsd:attribute>		
	</xsd:complexType>	
	
	
	
	<xsd:complexType name="ManPortableAirDefenseSystem">
		<xsd:annotation>
            <xsd:documentation> ПЗРК </xsd:documentation>
        </xsd:annotation>	
		<xsd:complexContent>
			<xsd:extension base="conflict:Unit">
				<xsd:sequence>
					<xsd:element name="visor" type="conflict:Radar">
						<xsd:annotation>
							<xsd:documentation> Средство обнаружения </xsd:documentation>
						</xsd:annotation>	
					</xsd:element>
					<xsd:element name="firing_channel" type="conflict:FiringChannel">
						<xsd:annotation>
							<xsd:documentation> Стрельбовой канал </xsd:documentation>
						</xsd:annotation>	
					</xsd:element>
					<xsd:element name="ammunition" type="conflict:Ammunition">
						<xsd:annotation>
							<xsd:documentation> Арсенал ПЗРК </xsd:documentation>
						</xsd:annotation>	
					</xsd:element>
				</xsd:sequence>				
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	
    
    <xsd:complexType name="Ship">
        <xsd:annotation>
            <xsd:documentation> Модель корабля </xsd:documentation>
        </xsd:annotation>		
        <xsd:complexContent>
            <xsd:extension base="conflict:Unit">
				<xsd:sequence>
					<xsd:element name="firing_channels" type="conflict:FiringChannel" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation> Стрельбовые каналы </xsd:documentation>
						</xsd:annotation>	
					</xsd:element>
					<xsd:element name="ammunition" type="conflict:Ammunition">
						<xsd:annotation>
							<xsd:documentation> Боекомплект с учетом ракет в стволе </xsd:documentation>
						</xsd:annotation>	
					</xsd:element>	
					<xsd:element name="radars" type="conflict:Radar" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation> Модель станции обнаружения </xsd:documentation>
						</xsd:annotation>	
					</xsd:element>	
					<xsd:element name="control_center" type="conflict:ControlCenter">
						<xsd:annotation>
                            <xsd:documentation> Локальный пункт управления </xsd:documentation>
                        </xsd:annotation>
					</xsd:element>                    
				</xsd:sequence>			
				<xsd:attribute name="hits_for_die" type="conflict:exPositiveInt" default="1">
					<xsd:annotation>
						<xsd:documentation> Количество попаданий, необходимое для потопления </xsd:documentation>
						<xsd:appinfo source="Xsd2CppGen">
							<genParam trancatedInResult="True"/>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="hits_for_break" type="conflict:exPositiveInt" default="1">
					<xsd:annotation>
						<xsd:documentation> Количество попаданий, необходимое для выхода из строя основных элементов </xsd:documentation>
						<xsd:appinfo source="Xsd2CppGen">
							<genParam trancatedInResult="True"/>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="repair_time" type="conflict:incPositiveDouble" default="180">
					<xsd:annotation>
						<xsd:documentation> Время для перезапуска систем после выхода из строя, сек </xsd:documentation>
						<xsd:appinfo source="Xsd2CppGen">
							<genParam trancatedInResult="True"/>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="sinking_time" type="conflict:incPositiveDouble" default="0">
					<xsd:annotation>
						<xsd:documentation> Время ухода под воду после уничтожения, сек </xsd:documentation>
						<xsd:appinfo source="Xsd2CppGen">
							<genParam trancatedInResult="True"/>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
        </xsd:complexContent>          
    </xsd:complexType>
	
	
	
	<xsd:complexType name="Car">
        <xsd:annotation>
            <xsd:documentation> Модель автомобиля  </xsd:documentation>
        </xsd:annotation>		
        <xsd:complexContent>
            <xsd:extension base="conflict:Unit">				
			</xsd:extension>
        </xsd:complexContent>          
    </xsd:complexType>
	
	
		

    <xsd:complexType name="Environment">
        <xsd:annotation>
            <xsd:documentation> Окружающая среда </xsd:documentation>
        </xsd:annotation>                   
		<xsd:sequence>
			<xsd:element name="weather" type="conflict:Weather">
				<xsd:annotation>
					<xsd:documentation> Модель погодных условий </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		 <xsd:attribute name="time" type="conflict:incPositiveDouble" default="0">
			<xsd:annotation>
				<xsd:documentation> Время моделирование </xsd:documentation>
				   <xsd:appinfo source="Xsd2CppGen">						
						<dialog show = "False"/>						
				   </xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="time_limit" type="conflict:exPositiveDouble" use="required">
			<xsd:annotation>
				<xsd:documentation> Ограничение времени моделирования, сек </xsd:documentation>
				<xsd:appinfo source="Xsd2CppGen">
					<genParam trancatedInResult="True"/>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="dtime" type="conflict:exPositiveDouble" use="required">
			<xsd:annotation>
				<xsd:documentation> Шаг моделирования, сек </xsd:documentation>
				<xsd:appinfo source="Xsd2CppGen">
					<genParam trancatedInResult="True"/>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>		
		<xsd:attribute name="zone_limit" type="conflict:exPositiveDouble" default="9999999">
			<xsd:annotation>
				<xsd:documentation> Радиус ограничивающей полусферы, м </xsd:documentation>				
			</xsd:annotation>
		</xsd:attribute>	
    </xsd:complexType>
	
	
	<xsd:complexType name="Randomizer">
		<xsd:annotation>
            <xsd:documentation> Генератор случайных чисел </xsd:documentation>
        </xsd:annotation>
		<xsd:attribute name="seed" type="xsd:int" default="-1">
			<xsd:annotation>
				<xsd:documentation> Зерно генератора. меньше 0 для случайного значения </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	
	
	
	<!--		BATTLE ORDERS				-->	
	
	
	
	<xsd:complexType name="BattleOrder" abstract="true">
		<xsd:annotation>
			<xsd:documentation> Базовый класс боевого простроения </xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="capacity" type="conflict:exPositiveInt" default="1">
			<xsd:annotation>
				<xsd:documentation> Число объектов в группе </xsd:documentation>
				<xsd:appinfo source="Xsd2CppGen">						
					<dialog show = "False"/>						
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	
	
	
	<xsd:complexType name="BattleOrderLine">
		<xsd:annotation>
			<xsd:documentation> Построение в ряд </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="conflict:BattleOrder">
				<xsd:attribute name="interval" type="conflict:incPositiveDouble">
					<xsd:annotation>
						<xsd:documentation> Расстояние между соседними объектами, м </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>				
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	
	
	<xsd:complexType name="BattleOrderColumn">
		<xsd:annotation>
			<xsd:documentation> Построение в колонну </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="conflict:BattleOrder">
				<xsd:attribute name="interval" type="conflict:incPositiveDouble">
					<xsd:annotation>
						<xsd:documentation> Расстояние между соседними объектами, м </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>				
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	
	
	<!--		BATTLE ORDERS END			-->	
	
	
	
	<xsd:complexType name="BattleGroup" abstract="true">
		<xsd:annotation>
			<xsd:documentation> Боевая группа </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="order" type="conflict:BattleOrder">
				<xsd:annotation>
					<xsd:documentation> Боевой порядок </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="center" type="conflict:Position">
				<xsd:annotation>
					<xsd:documentation> Центр группы </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="control_center" type="conflict:ControlCenter" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> Глобальный пункт управления </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="membership" type="conflict:Membership" use="required">
			<xsd:annotation>
				<xsd:documentation> Принадлежность к группе </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="course" type="xsd:double" default="0">
			<xsd:annotation>
				<xsd:documentation> Угол курса группы </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="slope" type="xsd:double" default="0">
			<xsd:annotation>
				<xsd:documentation> Угол наклона траектории группы </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="speed" type="xsd:double" default="0">
			<xsd:annotation>
				<xsd:documentation> Скорость движения группы </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	
	
	
	<xsd:complexType name="BattleGroupSample">
		<xsd:annotation>
			<xsd:documentation> Группа однотипных объектов </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="conflict:BattleGroup">
				<xsd:sequence>
					<xsd:element name="prototype" type="conflict:Unit">
						<xsd:annotation>
							<xsd:documentation> Прототип объектов группы </xsd:documentation>
							<xsd:appinfo source="Xsd2CppGen">
								<genParam>
									<prototype destListName="units" global="true"/>
								</genParam>
							</xsd:appinfo>
						</xsd:annotation>
					</xsd:element>					
				</xsd:sequence>
				<xsd:attribute name="size" type="conflict:exPositiveInt" use="required">
					<xsd:annotation>
						<xsd:documentation> Число объектов в группе </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>				
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	
	
	<xsd:complexType name="BattleGroupMulti">
		<xsd:annotation>
			<xsd:documentation> Группа объектов разного типа </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="conflict:BattleGroup">
				<xsd:sequence>
					<xsd:element name="units" type="conflict:Unit" minOccurs="1" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation> Объекты группы </xsd:documentation>
						</xsd:annotation>
					</xsd:element>					
				</xsd:sequence>				
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	

    <xsd:complexType name="Settings">
        <xsd:annotation>
            <xsd:documentation> Настройки моделирования </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>     
			<xsd:element name="randomizer" type="conflict:Randomizer">
				<xsd:annotation>
					<xsd:documentation> Настройка генератора случайных чисел </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="environment" type="conflict:Environment">
				<xsd:annotation>
					<xsd:documentation> Настройка окружающей среды </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="groups" type="conflict:BattleGroup" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation> Настройка боевых групп </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="units" type="conflict:Unit" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation> Вектор, хранящий прототипы клонируемых юнитов </xsd:documentation>
						<xsd:appinfo source="Xsd2CppGen">						
							<dialog show = "False"/>						
						</xsd:appinfo>					
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="missiles" type="conflict:Missile" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
						<xsd:documentation> Вектор, хранящий прототипы клонируемых ракет </xsd:documentation>
						<xsd:appinfo source="Xsd2CppGen">						
							<dialog show = "False"/>						
						</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="settings" type="conflict:Settings"/>
</xsd:schema>
