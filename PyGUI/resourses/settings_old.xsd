    <xsd:complexType name="TargetDistribution" abstract="true">
        <xsd:annotation>
            <xsd:documentation> Распределение целей </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="danger_factors" type="conflict:DangerFactor" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation> ранжирование критерия опасности </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="auto_r_dangers" type="xsd:boolean" default="false"/>
    </xsd:complexType>

    
    
    <xsd:complexType name="SaatiTD">
        <xsd:annotation>
            <xsd:documentation> Алгоритм ранжирования целей Саати </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="conflict:TargetDistribution"/>
        </xsd:complexContent>
    </xsd:complexType>

    
    
    <xsd:complexType name="ControlCenter">
        <xsd:annotation>
            <xsd:documentation> Командный пункт </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>            
            <xsd:element name="t_distr" type="conflict:TargetDistribution">
                <xsd:annotation>
                    <xsd:documentation> Целераспределение </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
		<xsd:attribute name="target_outdate" type="conflict:exPositiveDouble" default="10">
            <xsd:annotation>
                <xsd:documentation> Время для принятия решения об устаревании цели, сек </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    
    
    <xsd:complexType name="Weather">
        <xsd:annotation>
            <xsd:documentation> Погодные условия </xsd:documentation>
			<xsd:appinfo source="Xsd2CppGen">
				<genParam noInheritance="True"/>
			</xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="wind_speed" type="conflict:RVector">
                <xsd:annotation>
                    <xsd:documentation> Скорость ветра </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>        
        <xsd:attribute name="visibility" type="conflict:Percent" default="1">
            <xsd:annotation>
                <xsd:documentation> Видимость [0;1] </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="refraction" type="conflict:Percent" default="1">
            <xsd:annotation>
                <xsd:documentation> Преломление [0;1] </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="waves_height" type="conflict:incPositiveDouble" default="0">
            <xsd:annotation>
                <xsd:documentation> Высота волн </xsd:documentation>                
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
	
    
	
    <xsd:complexType name="Warhead">
        <xsd:annotation>
            <xsd:documentation> Модель боевой части </xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="explode_radius" type="conflict:incPositiveDouble" use="required">
            <xsd:annotation>
                <xsd:documentation> Радиус подрыва цели </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
		<xsd:attribute name="explode_chance" type="conflict:Percent" default="1">
            <xsd:annotation>
                <xsd:documentation> Вероятность подрыва цели [0;1] </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
		<xsd:attribute name="friendly_fire" type="xsd:boolean" default="true">
            <xsd:annotation>
                <xsd:documentation> Возможность поражения союзников при близком подрыве </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
  	
    

    
	
	
	<xsd:complexType name="Motion" abstract="true">
		<xsd:annotation>
            <xsd:documentation> Уравнения движения. Базовый тип </xsd:documentation>
        </xsd:annotation>
	</xsd:complexType>
	
	
		
	<xsd:complexType name = "MotionLoadable">
		<xsd:annotation>
            <xsd:documentation> Движение, загружаемое из файла </xsd:documentation>
        </xsd:annotation>
		<xsd:complexContent>
            <xsd:extension base="conflict:Motion">
				<xsd:attribute name="fname" type="conflict:DataFileName" use="required">
					<xsd:annotation>
						<xsd:documentation> Файл с траекторией </xsd:documentation>						
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	
	
	<xsd:complexType name = "MotionBalancing">
		<xsd:annotation>
            <xsd:documentation> Движение в балансировочном режиме </xsd:documentation>
        </xsd:annotation>
		<xsd:complexContent>
            <xsd:extension base="conflict:Motion">				
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
		
	
	
	<xsd:complexType name = "MotionSimple">
		<xsd:annotation>
            <xsd:documentation> Движение с постоянной скоростью, без учета двигателя </xsd:documentation>
        </xsd:annotation>
		<xsd:complexContent>
            <xsd:extension base="conflict:Motion">				
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	

	<xsd:complexType name = "Missile">
		<xsd:annotation>
            <xsd:documentation> Ракета </xsd:documentation>
			<xsd:appinfo source="Xsd2CppGen">
				<treeView icon = ":/conflicttreeview/images/missile.png"/>
			</xsd:appinfo>
        </xsd:annotation>
		<xsd:complexContent>
            <xsd:extension base="conflict:Unit">        
				<xsd:sequence>		
					<xsd:element name="motion" type="conflict:Motion">
						<xsd:annotation>
							<xsd:documentation> Уравнения движения </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="power_plant" type="conflict:PowerPlant">
						<xsd:annotation>
							<xsd:documentation> Двигательная установка </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="glider" type="conflict:Glider">
						<xsd:annotation>
							<xsd:documentation> Модель аэродинамики </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="cpu" type="conflict:CentralComputer">
						<xsd:annotation>
							<xsd:documentation> БЦВМ </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="head" type="conflict:HomingHead" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation> ГСН </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="sensors" type="conflict:MeasuringSystem">
						<xsd:annotation>
							<xsd:documentation> Модель ошибок измерительной системы </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="mass_ch" type="conflict:MassParameters" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation> МЦХ объекта </xsd:documentation>
							<xsd:appinfo source="Xsd2CppGen">
									<genParam trancatedInResult="True"/>
								</xsd:appinfo>
						</xsd:annotation>							
					</xsd:element>	
					<xsd:element name="aiming_point" type="conflict:RVector">
						<xsd:annotation>
							<xsd:documentation> Точка прицеливания </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="warhead" type="conflict:Warhead">
						<xsd:annotation>
							<xsd:documentation> Боеголовка </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="selfdestruction" type="conflict:SelfDestructionAlgorithm">
						<xsd:annotation>
							<xsd:documentation> Алгоритм самоликвидации </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="mass" type="conflict:incPositiveDouble" default="0">
					<xsd:annotation>
						<xsd:documentation> текущее значение массы </xsd:documentation>
                           <xsd:appinfo source="Xsd2CppGen">						
                                <dialog show = "False"/>						
                           </xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="xt" type="xsd:double" default="0">
					<xsd:annotation>
						<xsd:documentation> текущее положение ц.м. </xsd:documentation>
                            <xsd:appinfo source="Xsd2CppGen">						
                                <dialog show = "False"/>						
                            </xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="self_mass" type="conflict:exPositiveDouble" default="0.01">
					<xsd:annotation>
						<xsd:documentation> Масса ракеты (без двигателя) </xsd:documentation>
						<xsd:appinfo source="Xsd2CppGen">
							<genParam trancatedInResult="True"/>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="self_xt" type="conflict:incPositiveDouble" default="0">
					<xsd:annotation>
						<xsd:documentation> Положение ц.м. без двигателя </xsd:documentation>
						<xsd:appinfo source="Xsd2CppGen">
							<genParam trancatedInResult="True"/>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	
	
